{
    "name": "Internet Archive",
    "id": "internetarchive",
    "packaging_format": 1,
    "description": {
        "en": "Offline version of the Internet Archive",
        "fr": "Version hors ligne de l’Internet Archive",
        "mr": "इंटरनेट आर्काइव्हची ऑफलाइन आवृत्ती"
    },
    "version": "2022.04.13~ynh1",
    "url": "https://github.com/internetarchive/dweb-mirror",
    "upstream": {
        "license": "AGPL-3.0-only",
        "code": "https://github.com/internetarchive/dweb-mirror"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Mitra Ardron",
        "email": "mitra@mitra.biz",
        "url": "https://mitra.biz"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/internetarchive",
                "default": "/internetarchive"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
